print("jatin katyal")
print(1)

#Print can exhibit any number of argument
print(1,2,3,4,5,6)
print(1,2,3,"jatin",3.4,1+5j)
print(1,2,3,"jatin",3.4,1+5j,sep=",")

print("Hello")
print("World!")

print("a")
print("b")
print("c")

print("a",end=")")
print("b")
print("c")

print("Hello",end=";")
print("World!")
